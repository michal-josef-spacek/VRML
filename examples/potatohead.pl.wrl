Sound {
	direction	0 0 -1
	maxFront	100
	source DEF HALLO AudioClip {
		url		"hallo.wav"
		description	"HALLO"
		loop		FALSE
		pitch		2
	}
}
DEF MOVE
PositionInterpolator {
    key [ 0, .5, 1]
    keyValue [0 0 0, 0 1 0, 0 0 0]
}
DEF TURN
OrientationInterpolator {
    key [ 0, .2, .5, .7, 1]
    keyValue [0 0 1 0, 0 0 1 .5, 0 0 1 0, 0 0 1 .5, 0 0 1 0]
}
DEF TS1
TimeSensor {
	cycleInterval	0.5
}
DEF TS2
TimeSensor {
	cycleInterval	2
}
ROUTE HEAD-TOUCH.touchTime TO TS1.startTime
ROUTE HEAD-TOUCH.touchTime TO HALLO.startTime
ROUTE TS1.fraction_changed TO MOVE.set_fraction
ROUTE MOVE.value_changed TO HEAD.set_translation
ROUTE BODY-TOUCH.touchTime TO TS2.startTime
ROUTE BODY-TOUCH.touchTime TO HALLO.startTime
ROUTE TS2.fraction_changed TO TURN.set_fraction
ROUTE TURN.value_changed TO ARM.set_rotation
