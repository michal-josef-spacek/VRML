DEF HEAD
Transform {
    children [
	DEF HEAD-TOUCH TouchSensor {}
	Transform {
	    scale		4 3 2
	    children [
		Shape {
			appearance Appearance {
				material Material {
					emissiveColor	0.3 0.194117647058824 0	# orange%30
					diffuseColor	1 0.647058823529412 0	# orange
				}
			}
			geometry Sphere {
				radius	1
			}
		}
	    ]
	} # Transform
	Transform {
	    translation	0 4 0
	    children [
		Shape {
			appearance Appearance {
				material Material {
					diffuseColor	0 0 1	# blue
				}
			}
			geometry Cone {
				bottomRadius	2
				height	3
			}
		}
	    ]
	} # Transform
	Transform {
	    translation	-1 1 1
	    children [
		DEF eye
		Group {
		    children [
			Shape {
				appearance Appearance {
					material Material {
						diffuseColor	1 1 1	# white
					}
				}
				geometry Sphere {
					radius	0.9
				}
			}
			Transform {
			    translation	0 .1 .8
			    children [
				Shape {
					appearance Appearance {
						material Material {
							diffuseColor	0 0 0	# black
						}
					}
					geometry Sphere {
						radius	0.2
					}
				}
			    ]
			} # Transform
		    ]
		}
	    ]
	} # Transform
	Transform {
	    translation	1 1 1
	    children [
		USE eye
	    ]
	} # Transform
	Transform {
	    translation	0 -0.3 2.5
	    rotation	1 0 0 1.5707963
	    children [
		Shape {
			appearance Appearance {
				material Material {
					diffuseColor	1 0 0	# red
				}
			}
			geometry Cone {
				bottomRadius	0.5
				height	2
			}
		}
	    ]
	} # Transform
	Transform {
	    translation	0 -.4 0.7
	    rotation	1 0 0 1.39626337777778
	    children [
		Shape {
			appearance Appearance {
				material Material {
					diffuseColor	1 0 0	# red
				}
			}
			geometry Cylinder {
				radius	2
				height	1
			}
		}
	    ]
	} # Transform
    ]
} # Transform
Transform {
